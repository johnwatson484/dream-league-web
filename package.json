    "@hapi/good-squeeze": "^6.0.0",
    "@hapi/hapi": "^20.1.5",
    "@hapi/inert": "^6.0.3",
    "@hapi/vision": "^6.1.0",
    "hapi-auth-jwt2": "^10.2.0",
    "joi": "^17.4.1",
